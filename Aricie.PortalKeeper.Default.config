<PortalKeeperConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <FirewallConfig>
    <Name>Firewall Engine</Name>
    <Decription><![CDATA[This is the rule engine used to filter incoming Http Requests. Default rules help securing your DotNetNuke instance, and route specific requests such as mobile devices.]]></Decription>
    <Enabled>true</Enabled>
    <Variables>
      <Instances>
        <SubTypes />
        <ArrayOfVariableInfo />
      </Instances>
      <ShowAvailableTypes>false</ShowAvailableTypes>
      <ExpressionTypes>
        <DotNetType>
          <TypeName>System.String, mscorlib</TypeName>
        </DotNetType>
        <DotNetType>
          <TypeName>System.Boolean, mscorlib</TypeName>
        </DotNetType>
      </ExpressionTypes>
    </Variables>
  <Rules>
    <KeeperRuleOfRequestEvent>
      <Name>Anti-DoS Protection</Name>
        <Decription><![CDATA[This rule serves as the main Denial of Service detection. By default, it is configured with a request caps filter with anti-DoS enabled. The action is defaulted to a 403 forbidden status.
      
      TODO: You should edit the request caps according to your target environment.]]></Decription>
        <Enabled>true</Enabled>
      <MatchingLifeCycleEvent>BeginRequest</MatchingLifeCycleEvent>
      <StopRule>true</StopRule>
      <Condition>
        <Instances>
          <SubTypes>
            <string>Aricie.DNN.Modules.PortalKeeper.RequestsCapsCondition, Aricie.PortalKeeper</string>
          </SubTypes>
          <ArrayOfConditionProviderSettingsOfRequestEvent>
            <ConditionProviderSettingsOfRequestEvent xsi:type="RequestsCapsCondition">
              <Name>Main Request Caps</Name>
              <Decription><![CDATA[Those are limits put on the rate of requests sent to the server. The default caps include limits on all requests by IPs or by country, and additional constraints on DNN page requests]]></Decription>
                <Enabled>true</Enabled>
              <ProviderName>Requests caps Condition</ProviderName>
              <Negate>false</Negate>
              <IsMandatory>false</IsMandatory>
              <EnableDoSProtection>true</EnableDoSProtection>
              <DoSProtectionDuration d="00:05:00" />
              <BroadcastTaskQueueInfo>
                <TaksWaitTime d="00:00:00.1000000" />
                <InitialWaitTime d="00:00:01" />
                <WakeUpWaitTime d="00:00:01" />
                <NbThreads>1</NbThreads>
                <IsBackground>true</IsBackground>
              </BroadcastTaskQueueInfo>
              <RequestsCaps>
                <RequestsCapInfo>
                  <Enabled>true</Enabled>
                  <RequestSource>
                      <SourceType>XForwardedIP</SourceType>
                    <ExpressionFilter>
                      <MaxLength>-1</MaxLength>
                      <ForceToLower>false</ForceToLower>
                      <EncodePreProcessing>None</EncodePreProcessing>
                      <TransformList />
                    </ExpressionFilter>
                  </RequestSource>
                  <CappedRequestScope>Any</CappedRequestScope>
                  <MaxNbRequest>100</MaxNbRequest>
                  <MaxNbNewSources>0</MaxNbNewSources>
                  <Duration d="00:00:10" />
                </RequestsCapInfo>
                <RequestsCapInfo>
                  <Enabled>true</Enabled>
                  <RequestSource>
                      <SourceType>XForwardedIP</SourceType>
                    <ExpressionFilter>
                      <MaxLength>-1</MaxLength>
                      <ForceToLower>false</ForceToLower>
                      <EncodePreProcessing>None</EncodePreProcessing>
                      <TransformList />
                    </ExpressionFilter>
                  </RequestSource>
                  <CappedRequestScope>Any</CappedRequestScope>
                  <MaxNbRequest>250</MaxNbRequest>
                  <MaxNbNewSources>0</MaxNbNewSources>
                  <Duration d="00:01:00" />
                </RequestsCapInfo>
                <RequestsCapInfo>
                  <Enabled>true</Enabled>
                  <RequestSource>
                      <SourceType>XForwardedIP</SourceType>
                    <ExpressionFilter>
                      <MaxLength>-1</MaxLength>
                      <ForceToLower>false</ForceToLower>
                      <EncodePreProcessing>None</EncodePreProcessing>
                      <TransformList />
                    </ExpressionFilter>
                  </RequestSource>
                  <CappedRequestScope>DNNPageOnly</CappedRequestScope>
                  <MaxNbRequest>60</MaxNbRequest>
                  <MaxNbNewSources>0</MaxNbNewSources>
                  <Duration d="00:02:00" />
                </RequestsCapInfo>
                <RequestsCapInfo>
                  <Enabled>true</Enabled>
                  <RequestSource>
                    <SourceType>Country</SourceType>
                    <ExpressionFilter>
                      <MaxLength>-1</MaxLength>
                      <ForceToLower>false</ForceToLower>
                      <EncodePreProcessing>None</EncodePreProcessing>
                      <TransformList />
                    </ExpressionFilter>
                  </RequestSource>
                  <CappedRequestScope>Any</CappedRequestScope>
                  <MaxNbRequest>2000</MaxNbRequest>
                  <MaxNbNewSources>0</MaxNbNewSources>
                  <Duration d="00:01:00" />
                </RequestsCapInfo>
              </RequestsCaps>
            </ConditionProviderSettingsOfRequestEvent>
          </ArrayOfConditionProviderSettingsOfRequestEvent>
        </Instances>
      </Condition>
      <Action>
        <Instances>
          <SubTypes>
            <string>Aricie.DNN.Modules.PortalKeeper.RequestDenialAction, Aricie.PortalKeeper</string>
          </SubTypes>
          <ArrayOfActionProviderSettingsOfRequestEvent>
            <ActionProviderSettingsOfRequestEvent xsi:type="RequestDenialAction">
              <Name>Pre-Denial Response</Name>
              <Decription><![CDATA[This action will occur when the client agent triggers the limit cap. The default behavior is to throw an HttpException with a 403 status code. This supposes that the custom errors are correctly configured (see the appropriate section). If the Anti DoS system is enabled (true by default), this action will be run only once. During a ban, the subsequent requests will trigger the global default anti-DoS action.]]></Decription>
                <Enabled>true</Enabled>
              <ProviderName>Request Denial Action</ProviderName>
              <LifeCycleEvent>BeginRequest</LifeCycleEvent>
              <StopOnFailure>false</StopOnFailure>
                <ExitAction>false</ExitAction>
              <RethrowException>true</RethrowException>
              <DontLogExceptions>true</DontLogExceptions>
                <CaptureException>false</CaptureException>
                <ExceptionVarName>ActionException</ExceptionVarName>
                <AddSleepTime>false</AddSleepTime>
                <SleepTime d="00:00:00" />
                <ConditionalAction>false</ConditionalAction>
                <Condition>
                  <Instances>
                    <SubTypes />
                    <ArrayOfConditionProviderSettingsOfRequestEvent />
                  </Instances>
                </Condition>
                <AlternateAction>
                  <Instances>
                    <SubTypes />
                    <ArrayOfActionProviderSettingsOfRequestEvent />
                  </Instances>
                </AlternateAction>
                <CaptureRunDuration>false</CaptureRunDuration>
                <RunDurationVarName>ActionRunDuration</RunDurationVarName>
              <RequestDenialActionType>Send403</RequestDenialActionType>
              <RequestAbortType>CloseResponse</RequestAbortType>
              <RequestDenialActionMode>ThrowHttpException</RequestDenialActionMode>
              <ExceptionMessage />
                <RewriteContent />
            </ActionProviderSettingsOfRequestEvent>
          </ArrayOfActionProviderSettingsOfRequestEvent>
        </Instances>
      </Action>
    </KeeperRuleOfRequestEvent>
    <KeeperRuleOfRequestEvent>
      <Name>Critical Accounts Protection</Name>
        <Decription><![CDATA[This rule is meant to restrict the access of critical user accounts to a specific set of set of trusted locations.It is disabled by default because you first need to configure the trusted locations. Be careful before you apply it to set and note your recovery string because you might lock yourself out of the instance if that configuration is wrong.

TODO: You should edit the trusted location sub-conditions according to your requirements.]]></Decription>
        <Enabled>false</Enabled>
      <MatchingLifeCycleEvent>PreRequestHandlerExecute</MatchingLifeCycleEvent>
      <StopRule>true</StopRule>
      <Condition>
        <Instances>
          <SubTypes>
            <string>Aricie.DNN.Modules.PortalKeeper.MembershipCondition, Aricie.PortalKeeper</string>
            <string>Aricie.DNN.Modules.PortalKeeper.SubConditionsProvider`1[[Aricie.DNN.Modules.PortalKeeper.RequestEvent, Aricie.PortalKeeper]], Aricie.PortalKeeper</string>
          </SubTypes>
          <ArrayOfConditionProviderSettingsOfRequestEvent>
            <ConditionProviderSettingsOfRequestEvent xsi:type="MembershipCondition">
              <Name>Match Critical accounts</Name>
              <Decription><![CDATA[This condition defines the critical accounts to be protected.It is set to mandatory such that the location condition is not evaluated when the account is not critical.]]></Decription>
                <Enabled>true</Enabled>
              <ProviderName>Membership Condition</ProviderName>
              <Negate>false</Negate>
              <IsMandatory>true</IsMandatory>
              <MatchSuperUsers>true</MatchSuperUsers>
              <MatchRoles>Administrators</MatchRoles>
              <MatchUserIds />
            </ConditionProviderSettingsOfRequestEvent>
            <ConditionProviderSettingsOfRequestEvent xsi:type="SubConditionsProviderOfRequestEvent">
              <Name>Match Untrusted locations</Name>
              <Decription><![CDATA[This condition is negated so that it matches all locations outside of the inner set of locations. That set is defined by a series of sub-conditions to be configured. By default, 2 sub conditions are proposed with empty parameters: 1 for a set of trusted IP addresses, and 1  defining a set of trusted portal aliases.]]></Decription>
                <Enabled>true</Enabled>
              <ProviderName>Sub Conditions Provider</ProviderName>
              <Negate>true</Negate>
              <IsMandatory>true</IsMandatory>
              <EnableDoSProtection>false</EnableDoSProtection>
              <DoSProtectionDuration d="00:05:00" />
              <BroadcastTaskQueueInfo>
                <TaksWaitTime d="00:00:00.1000000" />
                <InitialWaitTime d="00:00:01" />
                <WakeUpWaitTime d="00:00:01" />
                <NbThreads>1</NbThreads>
                <IsBackground>true</IsBackground>
              </BroadcastTaskQueueInfo>
              <KeeperCondition>
                <Instances>
                  <SubTypes>
                    <string>Aricie.DNN.Modules.PortalKeeper.ClientSourceCondition, Aricie.PortalKeeper</string>
                    <string>Aricie.DNN.Modules.PortalKeeper.PortalAliasCondition, Aricie.PortalKeeper</string>
                  </SubTypes>
                  <ArrayOfConditionProviderSettingsOfRequestEvent>
                    <ConditionProviderSettingsOfRequestEvent xsi:type="ClientSourceCondition">
                      <Name>Trusted IP ranges</Name>
                      <Decription><![CDATA[This condition defines a set of trusted client IPs. It is empty by default.]]></Decription>
                        <Enabled>true</Enabled>
                      <ProviderName>Client Source Condition</ProviderName>
                      <Negate>false</Negate>
                      <IsMandatory>false</IsMandatory>
                      <EnableDoSProtection>false</EnableDoSProtection>
                      <DoSProtectionDuration d="00:05:00" />
                      <BroadcastTaskQueueInfo>
                        <TaksWaitTime d="00:00:00.1000000" />
                        <InitialWaitTime d="00:00:01" />
                        <WakeUpWaitTime d="00:00:01" />
                        <NbThreads>1</NbThreads>
                        <IsBackground>true</IsBackground>
                      </BroadcastTaskQueueInfo>
                      <RequestSource>
                          <SourceType>XForwardedIP</SourceType>
                        <ExpressionFilter>
                          <MaxLength>-1</MaxLength>
                          <ForceToLower>false</ForceToLower>
                          <EncodePreProcessing>None</EncodePreProcessing>
                          <TransformList />
                        </ExpressionFilter>
                      </RequestSource>
                      <ValueList>
                        <string>127.0.0.1</string>
                      </ValueList>
                      <RegexList />
                    </ConditionProviderSettingsOfRequestEvent>
                    <ConditionProviderSettingsOfRequestEvent xsi:type="PortalAliasCondition">
                      <Name>Trusted Alias</Name>
                      <Decription><![CDATA[That condition defines a set of trusted portal aliases (those not accessible from the Internet)]]></Decription>
                        <Enabled>true</Enabled>
                      <ProviderName>Portal Alias Condition</ProviderName>
                      <Negate>false</Negate>
                      <IsMandatory>false</IsMandatory>
                      <Items />
                    </ConditionProviderSettingsOfRequestEvent>
                      <ConditionProviderSettingsOfRequestEvent xsi:type="ClientSourceCondition">
                        <Name>Trusted Countries</Name>
                        <Decription><![CDATA[
                                                    If you're traveling and you can't know for sure where you'll connect from, you may still restrict the access to the countries you'll visit to prevent attacks from abroad. 
Add the corresponding values to the list and enable to grant country level accesses.
                                                ]]></Decription>
                        <Enabled>false</Enabled>
                        <ProviderName>Client Source Condition</ProviderName>
                        <Negate>false</Negate>
                        <IsMandatory>false</IsMandatory>
                        <EnableDoSProtection>false</EnableDoSProtection>
                        <DoSProtectionDuration d="00:05:00" />
                        <BroadcastTaskQueueInfo>
                          <TaksWaitTime d="00:00:00.1000000" />
                          <InitialWaitTime d="00:00:01" />
                          <WakeUpWaitTime d="00:00:01" />
                          <NbThreads>1</NbThreads>
                          <IsBackground>true</IsBackground>
                        </BroadcastTaskQueueInfo>
                        <RequestSource>
                          <SourceType>Country</SourceType>
                          <ExpressionFilter>
                            <MaxLength>-1</MaxLength>
                            <ForceToLower>false</ForceToLower>
                            <EncodePreProcessing>None</EncodePreProcessing>
                            <TransformList />
                          </ExpressionFilter>
                        </RequestSource>
                        <ValueList>
                          <string>N/A</string>
                          <string>France</string>
                        </ValueList>
                        <RegexList />
                      </ConditionProviderSettingsOfRequestEvent>
                  </ArrayOfConditionProviderSettingsOfRequestEvent>
                </Instances>
              </KeeperCondition>
            </ConditionProviderSettingsOfRequestEvent>
          </ArrayOfConditionProviderSettingsOfRequestEvent>
        </Instances>
      </Condition>
      <Action>
        <Instances>
          <SubTypes>
              <string>Aricie.DNN.Modules.PortalKeeper.LogEventAction`1[[Aricie.DNN.Modules.PortalKeeper.RequestEvent, Aricie.PortalKeeper]], Aricie.PortalKeeper</string>
            <string>Aricie.DNN.Modules.PortalKeeper.LogOffAction, Aricie.PortalKeeper</string>
            <string>Aricie.DNN.Modules.PortalKeeper.RedirectAction, Aricie.PortalKeeper</string>
          </SubTypes>
          <ArrayOfActionProviderSettingsOfRequestEvent>
              <ActionProviderSettingsOfRequestEvent xsi:type="LogEventActionOfRequestEvent">
                <Name>Add Admin Alert log</Name>
              <Decription><![CDATA[An event is added to the DNN event log, which in turn can be configured to send automated emails.]]></Decription>
                <Enabled>true</Enabled>
              <ProviderName>Log Event Action</ProviderName>
              <LifeCycleEvent>PreRequestHandlerExecute</LifeCycleEvent>
              <StopOnFailure>false</StopOnFailure>
                <ExitAction>false</ExitAction>
              <RethrowException>false</RethrowException>
              <DontLogExceptions>false</DontLogExceptions>
                <CaptureException>false</CaptureException>
                <ExceptionVarName>ActionException</ExceptionVarName>
                <AddSleepTime>false</AddSleepTime>
                <SleepTime d="00:00:00" />
                <ConditionalAction>false</ConditionalAction>
                <Condition>
                  <Instances>
                    <SubTypes />
                    <ArrayOfConditionProviderSettingsOfRequestEvent />
                  </Instances>
                </Condition>
                <AlternateAction>
                  <Instances>
                    <SubTypes />
                    <ArrayOfActionProviderSettingsOfRequestEvent />
                  </Instances>
                </AlternateAction>
                <CaptureRunDuration>false</CaptureRunDuration>
                <RunDurationVarName>ActionRunDuration</RunDurationVarName>
              <UseTaskQueue>false</UseTaskQueue>
              <TaskQueueInfo>
                <TaksWaitTime d="00:00:00.1000000" />
                <InitialWaitTime d="00:00:02" />
                <WakeUpWaitTime d="00:00:01" />
                <NbThreads>1</NbThreads>
                <IsBackground>true</IsBackground>
              </TaskQueueInfo>
              <Message>A client attempted to log in as a critical account user from an untrusted location.Rule Matched:"[Rule:Name]"; User Account:"[Context:Items:ClientUser:Username]"; Client IP:"[Context:DnnContext:IPAddress:ToString]" ([Context:DnnContext:CountryName])</Message>
              <EnableTokenReplace>true</EnableTokenReplace>
              <AdditionalTokenSource>
                <StaticTokens />
                <TokenProviders />
                <ConditionalTokens />
              </AdditionalTokenSource>
              <LogTypeKey>ADMIN_ALERT</LogTypeKey>
            </ActionProviderSettingsOfRequestEvent>
            <ActionProviderSettingsOfRequestEvent xsi:type="LogOffAction">
              <Name>Sign Untrusted User Off</Name>
              <Decription><![CDATA[Terminates the user session]]></Decription>
                <Enabled>true</Enabled>
              <ProviderName>Log Off Action</ProviderName>
              <LifeCycleEvent>PreRequestHandlerExecute</LifeCycleEvent>
              <StopOnFailure>false</StopOnFailure>
                <ExitAction>false</ExitAction>
              <RethrowException>false</RethrowException>
              <DontLogExceptions>false</DontLogExceptions>
                <CaptureException>false</CaptureException>
                <ExceptionVarName>ActionException</ExceptionVarName>
                <AddSleepTime>false</AddSleepTime>
                <SleepTime d="00:00:00" />
                <ConditionalAction>false</ConditionalAction>
                <Condition>
                  <Instances>
                    <SubTypes />
                    <ArrayOfConditionProviderSettingsOfRequestEvent />
                  </Instances>
                </Condition>
                <AlternateAction>
                  <Instances>
                    <SubTypes />
                    <ArrayOfActionProviderSettingsOfRequestEvent />
                  </Instances>
                </AlternateAction>
                <CaptureRunDuration>false</CaptureRunDuration>
                <RunDurationVarName>ActionRunDuration</RunDurationVarName>
            </ActionProviderSettingsOfRequestEvent>
            <ActionProviderSettingsOfRequestEvent xsi:type="RedirectAction">
              <Name>Redirect Unstrusted user</Name>
              <Decription><![CDATA[Redirects the infringing user to a dedicated landing page. It should be a dedicated DNN page where a text module warns the user for the infringement (default target is home page)]]></Decription>
                <Enabled>true</Enabled>
              <ProviderName>Redirect Action</ProviderName>
              <LifeCycleEvent>PreRequestHandlerExecute</LifeCycleEvent>
              <StopOnFailure>false</StopOnFailure>
                <ExitAction>false</ExitAction>
              <RethrowException>false</RethrowException>
              <DontLogExceptions>false</DontLogExceptions>
                <CaptureException>false</CaptureException>
                <ExceptionVarName>ActionException</ExceptionVarName>
                <AddSleepTime>false</AddSleepTime>
                <SleepTime d="00:00:00" />
                <ConditionalAction>false</ConditionalAction>
                <Condition>
                  <Instances>
                    <SubTypes />
                    <ArrayOfConditionProviderSettingsOfRequestEvent />
                  </Instances>
                </Condition>
                <AlternateAction>
                  <Instances>
                    <SubTypes />
                    <ArrayOfActionProviderSettingsOfRequestEvent />
                  </Instances>
                </AlternateAction>
                <CaptureRunDuration>false</CaptureRunDuration>
                <RunDurationVarName>ActionRunDuration</RunDurationVarName>
              <Target>
                  <Url>-1</Url>
                <Track>false</Track>
                <RedirectMode>ResponseRedirect</RedirectMode>
              </Target>
              <EndResponse>false</EndResponse>
            </ActionProviderSettingsOfRequestEvent>
          </ArrayOfActionProviderSettingsOfRequestEvent>
        </Instances>
      </Action>
    </KeeperRuleOfRequestEvent>
    <KeeperRuleOfRequestEvent>
      <Name>Simultaneous Connections Detection</Name>
      <Decription><![CDATA[This rule serves identifying cases of simultaneous connections of the same user account from different locations.By default, the new client is left through, the former client is automatically signed out and an event is logged.]]></Decription>
        <Enabled>true</Enabled>
      <MatchingLifeCycleEvent>PreRequestHandlerExecute</MatchingLifeCycleEvent>
      <StopRule>false</StopRule>
      <Condition>
        <Instances>
          <SubTypes>
            <string>Aricie.DNN.Modules.PortalKeeper.MultipleConnectionsCondition, Aricie.PortalKeeper</string>
          </SubTypes>
          <ArrayOfConditionProviderSettingsOfRequestEvent>
            <ConditionProviderSettingsOfRequestEvent xsi:type="MultipleConnectionsCondition">
              <Name>Catch Multiple connections</Name>
              <Decription><![CDATA[This condition matches the simultaneous connections. The discriminatory source is the Session by default.This prevents members from a customer's department to share a user account even if they share the same IP address. If you want only distinct IP addresses or even distinct countries to account for a match, update the corresponding parameter. Furthermore, the default condition is set such the only a returning user is matched (new users take over a flag).Also, no lock is preserved, such that the action is only performed once.]]></Decription>
                <Enabled>true</Enabled>
              <ProviderName>Multiple Connections Condition</ProviderName>
              <Negate>false</Negate>
              <IsMandatory>false</IsMandatory>
              <DiscriminationSource>
                  <SourceType>XForwardedIP</SourceType>
                <ExpressionFilter>
                  <MaxLength>-1</MaxLength>
                  <ForceToLower>false</ForceToLower>
                  <EncodePreProcessing>None</EncodePreProcessing>
                  <TransformList />
                </ExpressionFilter>
              </DiscriminationSource>
              <LockDuration d="01:00:00" />
                <NewConnectionsDontMatch>false</NewConnectionsDontMatch>
            </ConditionProviderSettingsOfRequestEvent>
          </ArrayOfConditionProviderSettingsOfRequestEvent>
        </Instances>
      </Condition>
      <Action>
        <Instances>
          <SubTypes>
              <string>Aricie.DNN.Modules.PortalKeeper.SendEmailAction`1[[Aricie.DNN.Modules.PortalKeeper.RequestEvent, Aricie.PortalKeeper]], Aricie.PortalKeeper</string>
            <string>Aricie.DNN.Modules.PortalKeeper.LogOffAction, Aricie.PortalKeeper</string>
          </SubTypes>
          <ArrayOfActionProviderSettingsOfRequestEvent>
              <ActionProviderSettingsOfRequestEvent xsi:type="SendEmailActionOfRequestEvent">
              <Name>Warn Ubiquist User by Email</Name>
              <Decription><![CDATA[The user being connected several times is warned by email.]]></Decription>
                <Enabled>true</Enabled>
              <ProviderName>Send Email Action</ProviderName>
              <LifeCycleEvent>PreRequestHandlerExecute</LifeCycleEvent>
              <StopOnFailure>false</StopOnFailure>
                <ExitAction>false</ExitAction>
              <RethrowException>false</RethrowException>
              <DontLogExceptions>false</DontLogExceptions>
                <CaptureException>false</CaptureException>
                <ExceptionVarName>ActionException</ExceptionVarName>
                <AddSleepTime>false</AddSleepTime>
                <SleepTime d="00:00:00" />
                <ConditionalAction>false</ConditionalAction>
                <Condition>
                  <Instances>
                    <SubTypes />
                    <ArrayOfConditionProviderSettingsOfRequestEvent />
                  </Instances>
                </Condition>
                <AlternateAction>
                  <Instances>
                    <SubTypes />
                    <ArrayOfActionProviderSettingsOfRequestEvent />
                  </Instances>
                </AlternateAction>
                <CaptureRunDuration>false</CaptureRunDuration>
                <RunDurationVarName>ActionRunDuration</RunDurationVarName>
              <UseTaskQueue>false</UseTaskQueue>
              <TaskQueueInfo>
                <TaksWaitTime d="00:00:00.1000000" />
                <InitialWaitTime d="00:00:02" />
                <WakeUpWaitTime d="00:00:01" />
                <NbThreads>1</NbThreads>
                <IsBackground>true</IsBackground>
              </TaskQueueInfo>
              <Message>We have just detected that on the portal "[Context:DnnContext:Portal:PortalName]", your account "[Context:Items:ClientUser:Username]" was connected simultaneously from two distinct locations. The following locations were last recorded: Client IP n°1:"[Context:Items:FirstClientSource]". Client IP n°2:"[Context:Items:LastClientSource]". If you think that was an identity theft, please contact urgently the portal administrator.</Message>
              <EnableTokenReplace>true</EnableTokenReplace>
              <AdditionalTokenSource>
                <StaticTokens />
                <TokenProviders />
                <ConditionalTokens />
              </AdditionalTokenSource>
                <FromAddress />
              <ToAddresses>[Context:Items:ClientUser:Email]</ToAddresses>
              <EmailObject>Multiple Connection detected for [Context:Items:ClientUser:Username]</EmailObject>
              <EmailPriority>High</EmailPriority>
              <BodyFormat>Text</BodyFormat>
                <Encoding>Default</Encoding>
            </ActionProviderSettingsOfRequestEvent>
            <ActionProviderSettingsOfRequestEvent xsi:type="LogOffAction">
              <Name>Sign Original User Off</Name>
              <Decription><![CDATA[Terminate the original user session]]></Decription>
                <Enabled>true</Enabled>
              <ProviderName>Log Off Action</ProviderName>
              <LifeCycleEvent>PreRequestHandlerExecute</LifeCycleEvent>
              <StopOnFailure>false</StopOnFailure>
                <ExitAction>false</ExitAction>
              <RethrowException>false</RethrowException>
              <DontLogExceptions>false</DontLogExceptions>
                <CaptureException>false</CaptureException>
                <ExceptionVarName>ActionException</ExceptionVarName>
                <AddSleepTime>false</AddSleepTime>
                <SleepTime d="00:00:00" />
                <ConditionalAction>false</ConditionalAction>
                <Condition>
                  <Instances>
                    <SubTypes />
                    <ArrayOfConditionProviderSettingsOfRequestEvent />
                  </Instances>
                </Condition>
                <AlternateAction>
                  <Instances>
                    <SubTypes />
                    <ArrayOfActionProviderSettingsOfRequestEvent />
                  </Instances>
                </AlternateAction>
                <CaptureRunDuration>false</CaptureRunDuration>
                <RunDurationVarName>ActionRunDuration</RunDurationVarName>
            </ActionProviderSettingsOfRequestEvent>
          </ArrayOfActionProviderSettingsOfRequestEvent>
        </Instances>
      </Action>
    </KeeperRuleOfRequestEvent>
    <KeeperRuleOfRequestEvent>
      <Name>Redirect Mobile Devices</Name>
        <Decription><![CDATA[This rule detects mobile browsers and redirects them to a dedicated subset of mobile pages.

TODO: You should update the set of mobile pages and the redirect target page, according to your actual mobile sub space.]]></Decription>
        <Enabled>false</Enabled>
      <MatchingLifeCycleEvent>BeginRequestAfterDNN</MatchingLifeCycleEvent>
      <StopRule>false</StopRule>
      <Condition>
        <Instances>
          <SubTypes>
            <string>Aricie.DNN.Modules.PortalKeeper.FleeExpressionCondition`1[[Aricie.DNN.Modules.PortalKeeper.RequestEvent, Aricie.PortalKeeper]], Aricie.PortalKeeper</string>
            <string>Aricie.DNN.Modules.PortalKeeper.DnnPageCondition, Aricie.PortalKeeper</string>
          </SubTypes>
          <ArrayOfConditionProviderSettingsOfRequestEvent>
            <ConditionProviderSettingsOfRequestEvent xsi:type="FleeExpressionConditionOfRequestEvent">
              <Name>Detect Mobile devices</Name>
                <Decription><![CDATA[That condition matches mobile clients according to the request headers. 
It is entirely made of a compound dynamic expression, and illustrates fully leveraging the underlying Expression Engine.]]></Decription>
                <Enabled>true</Enabled>
              <ProviderName>Dynamic Expression Condition</ProviderName>
              <Negate>false</Negate>
              <IsMandatory>true</IsMandatory>
              <FleeExpression>
