                                  <Template>
                                    <![CDATA[String.prototype.endsWith = function (suffix) {
   return this.indexOf(suffix, this.length - suffix.length) !== -1;
};
function RenderImagePickers() {
   $(".urlControl").each(function () {
      var urlc = $(this);
      var folder = urlc.find("select[id$=_cboFolders]").find('option:selected').text();
      var files = urlc.find("select[id$=_cboFiles]");
      var filesfound = false;
      files.find("option").each(function () {
         var filename = $(this).text();
         if (filename.toUpperCase().endsWith(".JPG") ||
            filename.toUpperCase().endsWith(".JPEG") ||
            filename.toUpperCase().endsWith(".PNG") ||
            filename.toUpperCase().endsWith(".GIF")) {
            $(this).data("img-src", "/portals/0/" +
              (folder == "Root" ? "/" : folder) + filename);
            filesfound = true;
         }
      });
      if (filesfound) {
         files.imagepicker({
            hide_select: false,
            show_label: false
         });
      }
   });
}
$(document).ready(function () {
   RenderImagePickers();
   Sys.WebForms.PageRequestManager.getInstance().add_endRequest(function () {
      RenderImagePickers();
   });
});]]>
